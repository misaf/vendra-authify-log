{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "misaf/vendra-authify-log",
    "description": "",
    "type": "vendra-module",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filamentphp",
        "authify",
        "log"
    ],
    "authors": [
        {
            "name": "Ehsan Mahmoodi",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "misaf/vendra-tenant": "^12.0",
        "misaf/vendra-user": "^12.0",
        "misaf/laravel-authify-log": "^1.0",
        "mokhosh/filament-jalali": "^5.1",
        "spatie/eloquent-sortable": "^4.5.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "filament/filament": "^4.7.0",
        "larastan/larastan": "^3.9.2",
        "laravel/boost": "^2.0.6",
        "laravel/pint": "^1.27",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.3.2",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-profanity": "^4.2.1",
        "pestphp/pest-plugin-type-coverage": "^4.0.3"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "pest"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\VendraAuthifyLog\\": "src/",
            "Misaf\\VendraAuthifyLog\\Tests\\": "tests/",
            "Misaf\\VendraAuthifyLog\\Database\\Factories\\": "database/factories/",
            "Misaf\\VendraAuthifyLog\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\VendraAuthifyLog\\Providers\\AuthifyLogServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ]
}
